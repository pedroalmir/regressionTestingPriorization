<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE jheat-mapping PUBLIC 
	"-//JHeat/JHeat DTD 1.0//BR" 
	"http://www.infoway-pi.com.br/jheat.dtd">
	
<jheat-mapping>
	
	<class-mapping class="br.com.infowaypi.ecare.programaPrevencao.SeguradoDoEvento" 
		name="seguradoDoEvento" display-name="Segurado do Programa" id-name="idSeguradoDoEvento">

	    <property name="nome" type="string" not-null="false">
			<display name="Nome do beneficiário" options="listing,detail-only"   maxlength="6" align="center" size="24"/>
	    </property>  

		<property name="numeroDoCartao" type="string" not-null="false">
			<display name="Número do Cartão" options="listing" mask="99.9999999999.99.9-9"  maxlength="6" align="center" size="24"/>
	    </property>  
		
         <property name="cpf" type="string" not-null="false">
			<display name="CPF" options="" mask="999.999.999-99" maxlength="6" align="center" size="24"/>
	    </property>  
        
	</class-mapping>

</jheat-mapping>