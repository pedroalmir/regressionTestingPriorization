<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE jheat-mapping PUBLIC 
	"-//JHeat/JHeat DTD 1.0//BR" 
	"http://www.infoway-pi.com.br/jheat.dtd">
	
<jheat-mapping>
	
	<class-mapping class="br.com.infowaypi.ecare.programaPrevencao.Evento" 
		name="evento" display-name="Evento" id-name="idEvento">
		
		<property name="titulo" type="string">
			<display options="listing" name="Titulo"/>
		</property>
		
		<property name="descricao" type="string">
			<display options="listing" name="Descrição"/>
		</property>
		
		<enum-property name="tipoDescricao" display-name="Tipo" class="br.com.infowaypi.ecare.enums.TipoEventoEnum" options="listing" display="descricao" value="descricao"/>
		
		<property name="data" type="date">
			<display options="listing" name="Data"/>
		</property>
		
		<role name="auditor" insert="false" exec="false" update="false" read="true" write="false"/>
	</class-mapping>

</jheat-mapping>