<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE jheat-mapping PUBLIC 
	"-//JHeat/JHeat DTD 1.0//BR" 
	"http://www.infoway-pi.com.br/jheat.dtd">
	
<jheat-mapping>
	
	<class-mapping class="br.com.infowaypi.ecarebc.atendimentos.RegistroTecnicoDaAuditoria" name="RegistroTecnicoDaAuditoria" 
		display-name="Registros Técnicos Da Auditoria" id-name="idObservacao" link-for-detail="dataCriacao" type="simple" section="">
		
		<description>Registros Técnicos Da Auditoria Externa</description>		
	    
	    <property name="titulo" type="string" not-null="true">
			<display name="Título" options="listing,search"/>
	    </property>
		
		<property name="texto" type="text" not-null="false">
			<display name="Texto" options="listing,search"/>
	    </property>
	    
	    <property name="dataDeCriacao" type="date">
	    	<display name="Data" options="listing, detail-only"/>
	    </property>
	    
		<validate-method name="validate"/>
	</class-mapping>

</jheat-mapping>